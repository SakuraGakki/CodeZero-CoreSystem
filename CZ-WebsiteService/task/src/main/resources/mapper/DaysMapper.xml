<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codezero.task.dao.DaysMapper">

    <insert id="insert" parameterType="com.codezero.task.pojo.days.Days">
        insert into days
        (date,week,status_desc,animal,avoid,`desc`,gz_date,gz_month,gz_year,l_date,l_month,suit)
        values
        (#{date},#{week},#{statusDesc},#{animal},#{avoid},#{desc},#{gzDate},#{gzMonth},#{gzYear},#{lDate},#{lMonth},#{suit})
    </insert>

</mapper>